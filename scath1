const int pinRed=10;
const int pinGreen=11;
const int pinYellw=12;
const int pinRed2 = 7;
const int pinGreen2 = 8;
const int pinYellw2 = 9;
int HIGHYellw = 500;
int LOWYellw = 500;
void setup(){
  pinMode(pinRed,OUTPUT);
  pinMode(pinGreen,OUTPUT);
  pinMode(pinYellw,OUTPUT);
  pinMode(pinRed2,OUTPUT);
  pinMode(pinGreen2,OUTPUT);
  pinMode(pinYellw2,OUTPUT);
}
void loop(){
  digitalWrite(pinRed,HIGH);
  digitalWrite(pinGreen,LOW);
  digitalWrite(pinYellw,LOW);
  digitalWrite(pinRed2,LOW);
  digitalWrite(pinGreen2,HIGH);
  digitalWrite(pinYellw2,LOW);
  delay(3000);

  digitalWrite(pinRed,LOW);
  digitalWrite(pinGreen,HIGH);
  digitalWrite(pinYellw,LOW);
  digitalWrite(pinRed2,HIGH);
  digitalWrite(pinGreen2,LOW);
  digitalWrite(pinYellw2,LOW);
  delay(3000);
  
  digitalWrite(pinRed,LOW);
  digitalWrite(pinRed2,LOW);
  digitalWrite(pinGreen,LOW);
  digitalWrite(pinGreen2,LOW);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
  digitalWrite(pinYellw2,HIGH);
  digitalWrite(pinYellw,LOW);
  delay(HIGHYellw);
  digitalWrite(pinYellw2,LOW);
  digitalWrite(pinYellw,HIGH);
  delay(LOWYellw);
}
